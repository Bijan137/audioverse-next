{
  "name": "audioverse-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "NODE_ICU_DATA=node_modules/full-icu next build",
    "build:dev": "ENV_OVERRIDE=development NODE_ICU_DATA=node_modules/full-icu next build --debug",
    "checkTs": "tsc --noEmit",
    "format": "prettier --write .",
    "lint": "eslint --max-warnings 0 --ext .ts --ext .tsx --ext .js --ext .jsx src/",
    "lint:fix": "eslint --max-warnings 0 --fix --ext .ts --ext .tsx --ext .js --ext .jsx src/",
    "start": "NODE_ICU_DATA=node_modules/full-icu next start",
    "test": "TZ=UTC jest",
    "test:watch": "TZ=UTC jest --watch",
    "test:strict": "TZ=UTC jest --config=jest.config.strict.js",
    "extract": "formatjs extract 'src/**/*.ts*' --ignore 'src/**/*.d.ts*' --throws --extract-source-location --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file public/lang/en.json",
    "compile": "formatjs compile-folder --ast public/lang public/compiled-lang",
    "precommit": "npm run format && npm run lint:fix && npm run checkTs",
    "postinstall": "husky install",
    "graphql:codegen": "graphql-codegen --config .graphqlrc.yml",
    "lighthouse": "npx lhci autorun",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "clsx": "^1.1.1",
    "cookie": "^0.4.1",
    "dayjs": "^1.10.6",
    "feed": "^4.2.1",
    "framer-motion": "^4.1.17",
    "full-icu": "^1.3.1",
    "graphql": "^15.4.0",
    "inter-ui": "^3.18.1",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.20",
    "masonic": "^3.4.1",
    "next": "^11.0.1",
    "next-pwa": "^5.2.24",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-facebook-login": "^4.1.1",
    "react-google-login": "^5.2.2",
    "react-intl": "^5.8.3",
    "react-query": "^3.5.5",
    "react-toastify": "^6.2.0",
    "react-tooltip": "^4.2.11",
    "react-topbar-progress-indicator": "^4.1.0",
    "sass": "^1.26.10",
    "type-fest": "^1.2.0",
    "video.js": "^7.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@formatjs/cli": "^2.12.0",
    "@graphql-codegen/cli": "^2.0.1",
    "@graphql-codegen/typescript": "^2.0.0",
    "@graphql-codegen/typescript-operations": "^2.0.1",
    "@graphql-codegen/typescript-react-query": "^2.0.0",
    "@lhci/cli": "^0.7.1",
    "@mizdra/eslint-plugin-layout-shift": "^1.0.1",
    "@next/eslint-plugin-next": "^11.0.1",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/dom": "^7.24.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.2",
    "@testing-library/user-event": "^12.1.3",
    "@types/cookie": "^0.4.0",
    "@types/jest-when": "^2.7.2",
    "@types/js-cookie": "^2.2.6",
    "@types/lodash": "^4.14.161",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^17.0.7",
    "@types/react-facebook-login": "^4.1.3",
    "@types/react-is": "^17.0.0",
    "@types/react-query": "^1.1.2",
    "@types/video.js": "^7.3.11",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-react-intl": "^8.2.4",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-formatjs": "^2.17.4",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jest-dom": "^3.9.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-testing-library": "^3.10.2",
    "graphql-tag": "^2.11.0",
    "husky": "^5.0.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jest-canvas-mock": "^2.3.0",
    "jest-when": "^3.3.1",
    "prettier": "^2.3.2",
    "ts-jest": "^26.3.0",
    "tsconfig-paths-jest": "^0.0.1",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.3.5"
  }
}