{
  "name": "audioverse-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "NODE_ICU_DATA=node_modules/full-icu next build",
    "format": "prettier --write .",
    "lint": "eslint --max-warnings 0 --ext .ts --ext .tsx --ext .js --ext .jsx src/",
    "lint:fix": "eslint --max-warnings 0 --fix --ext .ts --ext .tsx --ext .js --ext .jsx src/",
    "start": "next start",
    "test": "TZ=UTC jest",
    "test:watch": "TZ=UTC jest --watch",
    "extract": "formatjs extract 'src/**/*.tsx' --throws --extract-source-location --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file public/lang/en.json",
    "compile": "formatjs compile-folder --ast public/lang public/compiled-lang",
    "precommit": "npm run lint:fix && TZ=UTC jest --config=jest.config.precommit.js",
    "postinstall": "husky install",
    "graphql:codegen": "graphql-codegen --config .graphqlrc.yml"
  },
  "dependencies": {
    "@types/cookie": "^0.4.0",
    "@types/js-cookie": "^2.2.6",
    "@types/lodash": "^4.14.161",
    "@types/react-query": "^1.1.2",
    "@types/video.js": "^7.3.11",
    "cookie": "^0.4.1",
    "feed": "^4.2.1",
    "full-icu": "^1.3.1",
    "graphql": "^15.4.0",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.20",
    "next": "^10.0.0",
    "next-pwa": "^3.1.4",
    "react": "^16.13.1",
    "react-dom": "16.13.1",
    "react-intl": "^5.8.3",
    "react-query": "^3.5.5",
    "react-toastify": "^6.2.0",
    "react-tooltip": "^4.2.11",
    "react-topbar-progress-indicator": "^4.1.0",
    "sass": "^1.26.10",
    "video.js": "^7.10.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@formatjs/cli": "^2.12.0",
    "@graphql-codegen/cli": "1.20.0",
    "@graphql-codegen/typescript": "1.20.0",
    "@graphql-codegen/typescript-operations": "1.17.13",
    "@graphql-codegen/typescript-react-query": "^1.0.0",
    "@testing-library/dom": "^7.24.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.2",
    "@testing-library/user-event": "^12.1.3",
    "@types/jest-when": "^2.7.2",
    "@types/react": "^16.9.49",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-jest": "^26.3.0",
    "babel-plugin-react-intl": "^8.2.4",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-formatjs": "^2.7.12",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest-dom": "^3.2.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-testing-library": "^3.9.2",
    "graphql-tag": "^2.11.0",
    "husky": "^5.0.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jest-canvas-mock": "^2.3.0",
    "jest-when": "^3.1.0",
    "prettier": "^2.1.1",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  }
}
